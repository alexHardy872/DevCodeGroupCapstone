@model IEnumerable<DevCodeGroupCapstone.Models.TeacherAvail>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<p>
    @Html.ActionLink("Add Availability", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.weekDay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.start)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.end)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.weekDay)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.start)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.end)
    </td>

    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.availId }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.availId })
    </td>
</tr>
}

</table>
